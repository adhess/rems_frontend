{
  "root": "build/",
  "routes": {
    "/**": "index.html"
  },
  "proxies": {
    "/api": {
      "origin": "https://zland-backend.herokuapp.com",
      "changeOrigin": true,
      "pathRewrite": {
        "^/api": "/"
      }
    },
    "/findAddress": {
      "origin": "http://nominatim.openstreetmap.org",
      "changeOrigin": true,
      "pathRewrite": {
        "^/findAddress": "/"
      }
    }
  }
}